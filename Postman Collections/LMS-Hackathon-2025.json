					"name": "CreateProgram",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "3841ae6f-ce54-4dd5-a65c-fdbf2b1d05a5",
								"exec": [
									"let response = pm.response.json();\r",
									"let expected = JSON.parse(pm.iterationData.get(\"ExpectedResponseKeys\"));\r",
									"\r",
									"if (pm.response.code === 201) {\r",
									"    pm.test(\"Status code is 201\", function () {\r",
									"        pm.expect(pm.response.code).to.equal(201);\r",
									"        pm.collectionVariables.set(\"createProgramRespCd\", true);\r",
									"    });\r",
									"\r",
									"    pm.test(\"Response contains program details\", function () {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData).to.have.property('programName');\r",
									"        pm.expect(jsonData).to.have.property('programDescription');\r",
									"        pm.expect(jsonData).to.have.property('programStatus');\r",
									"    });\r",
									"\r",
									"    pm.test(\"Program status is valid\", function () {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData.programStatus).to.equal('Active');\r",
									"    });\r",
									"\r",
									"    pm.collectionVariables.set(\"programId\", response.programId);\r",
									"    pm.collectionVariables.set(\"programName\", response.programName);\r",
									"    pm.collectionVariables.set(\"programDescription\", response.programDescription);\r",
									"    pm.collectionVariables.set(\"programStatus\", response.programStatus);\r",
									"    pm.collectionVariables.set(\"creationTime\", response.creationTime);\r",
									"    pm.collectionVariables.set(\"lastModTime\", response.lastModTime);\r",
									"\r",
									"    pm.test(\"Program name is not empty\", function () {\r",
									"        const jsonData = pm.response.json();\r",
									"        pm.expect(jsonData.programName).to.be.a('string').and.not.empty;\r",
									"    });\r",
									"}\r",
									"\r",
									"pm.test(\"Response has valid structure\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.be.an('object');\r",
									"});\r",
									"\r",
									"if (pm.response.code !== 201) {\r",
									"    pm.test(\"Validate message and success fields\", function () {\r",
									"        pm.expect(response.message).to.eql(expected.message);\r",
									"        pm.expect(response.success).to.eql(expected.success);\r",
									"    });\r",
									"}\r",
									"\r",
									"pm.collectionVariables.set(\"createProgramExecuted\", true);"
